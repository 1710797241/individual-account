<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.models.dao.UserDao">
	<select id="selectAll" resultType="com.models.entity.User">
		select user_identity,
		user_name,
		user_sex,
		user_birth,
		user_phone,
		user_address,
		user_level,
		user_label
		from user
	</select>
	<select id="selectNameAndID" resultType="com.models.entity.User">
		select user_identity,
		user_name
		
		from user
	</select>
	<insert id="insertMany" parameterType="com.dto.UserDto">
		insert into user1
		(name.email,phone)
		values
		<foreach collection="userList" item="user">
			(#{user.name},#{user.email},#{user.phone})
		</foreach>
	</insert>
</mapper>